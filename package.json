{
  "name": "js-excel-template",
  "version": "2.0.1",
  "description": "A js excel template used in browser or nodejs environment.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p src/tsconfig.nodejs.json && tsc -p src/tsconfig.browser.json",
    "tslint": "tslint \"*.ts\" \"spec/*.ts\"",
    "test": "tsc -p spec && jasmine",
    "build": "npm run clean && npm run tsc && npm run rollup && tsc -p demo && npm run webpack",
    "rollup": "rollup --config rollup.config.js",
    "lint": "npm run tslint && npm run standard",
    "release": "clean-release",
    "webpack": "webpack --config demo/webpack.config.js",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\"",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/js-excel-template.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/js-excel-template/issues"
  },
  "homepage": "https://github.com/plantain-00/js-excel-template#readme",
  "devDependencies": {
    "@types/file-saver": "0.0.1",
    "@types/jasmine": "2.5.53",
    "clean-release": "1.3.1",
    "file-saver": "1.3.3",
    "jasmine": "2.6.0",
    "rimraf": "2.6.1",
    "rollup": "0.45.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "standard": "10.0.2",
    "tslint": "5.5.0",
    "typescript": "2.4.0",
    "webpack": "3.2.0"
  },
  "dependencies": {
    "xlsx": "0.10.8"
  }
}
